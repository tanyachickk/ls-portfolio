.feedback-section {
  position: relative;
  padding: 50px;
  width: 100%;
  height: 100vh;
  background-image: url('~images/content/autumn.jpg');
  background-size: cover;
  background-position: 0 0;
   &::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    width: 1375px;
    background-image: linear-gradient(to right, #1c2024 0%, #1c2023 42%, rgba(27, 31, 34, 0) 100%);
    opacity: 0.9;
  }
}